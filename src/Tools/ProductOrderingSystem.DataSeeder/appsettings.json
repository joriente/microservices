{
  "Seeding": {
    "Enabled": true,
    "ClearExistingData": true,
    "PublishEvents": false,
    "Services": {
      "ProductService": {
        "Enabled": true,
        "ProductCount": 100,
        "PublishEvents": true
      },
      "IdentityService": {
        "Enabled": true,
        "CreateAdminUser": true,
        "CreateShopperUser": true,
        "PublishEvents": true
      }
    }
  },
  "ConnectionStrings": {
    "messaging": "Endpoint=sb://localhost;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=SAS_KEY_VALUE;UseDevelopmentEmulator=true",
    "MongoDB": "mongodb://localhost:27017"
  },
  "MongoDB": {
    "ProductDatabaseName": "productdb",
    "IdentityDatabaseName": "identitydb",
    "OrderDatabaseName": "orderdb",
    "CartDatabaseName": "cartdb",
    "PaymentDatabaseName": "paymentdb",
    "CustomerDatabaseName": "customerdb"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "MassTransit": "Warning"
    }
  }
}
